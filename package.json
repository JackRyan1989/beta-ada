{
  "name": "federalist-uswds-jekyll",
  "version": "1.3.0",
  "scripts": {
    "build": "bundle exec jekyll build",
    "clean": "bundle exec jekyll clean",
    "start": "bundle exec jekyll serve --livereload",
    "test": "npm-run-all --continue-on-error --parallel test:*",
    "test:a11y": "pa11y-ci --sitemap http://127.0.0.1:4000/sitemap.xml",
    "test:html": "html-validate '_site/**/*.html'"
  },
  "devDependencies": {
    "html-validate": "^4.10.1",
    "npm-run-all": "^4.1.5",
    "pa11y-ci": "^2.4.0"
  }
}
